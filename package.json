{
  "name": "amazon-multiple-carts",
  "version": "1.0.0",
  "description": "Un pequeño proyecto personal para poder tener múltiples \"carritos\" de compra de Amazon.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run build:css\"",
    "dev": "nodemon index.js",
    "build:css": "npx tailwindcss -i ./public/css/tailwind.css -o ./public/css/main.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexDeveloperUwU/amazon-multiple-carts.git"
  },
  "keywords": [
    "Amazon"
  ],
  "author": "AlexDeveloperUwU",
  "bugs": {
    "url": "https://github.com/AlexDeveloperUwU/amazon-multiple-carts/issues"
  },
  "homepage": "https://github.com/AlexDeveloperUwU/amazon-multiple-carts#readme",
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.19",
    "ejs": "^3.1.10",
    "enmap": "^6.0.2",
    "express": "^4.19.2",
    "postcss": "^8.4.38"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "tailwindcss": "^3.4.4"
  }
}
